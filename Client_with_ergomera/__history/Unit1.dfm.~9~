  object vol500: TIBQuery
    Database = IBDatabase1
    Transaction = IBTransaction2
    BufferChunks = 1000
    CachedUpdates = False
    ParamCheck = True
    SQL.Strings = (
      'select coalesce(sum(NS5.VOLUME_2),0) as dvol_25, '
      
        'coalesce(sum(NS5.VOLUME_3),0) as dvol_35, (coalesce(sum(NS5.VOLU' +
        'ME_3),0) - coalesce(sum(NS5.VOLUME_2),0)) as dvol_5'
      'from GRAFIK_T500(:INTERVAL, :TIME_END) NS5_xy;')
